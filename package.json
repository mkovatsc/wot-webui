{
  "name": "wotwebui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --docs --env dev",
    "build:prod": "rimraf dist && webpack --bail --progress --profile --docs --env prod",
    "start": "npm-run-all --parallel serve",
    "serve": "webpack-dev-server --history-api-fallback --inline --progress --hot --env dev",
    "serve:prod": "webpack-dev-server --history-api-fallback --inline --progress --hot --env prod",
    "test": "npm run lint && karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "lint": "eslint src/ && htmlhint src/ && csslint src/styles/"
  },
  "dependencies": {
    "angular": "^1.6.6",
    "angular-animate": "^1.6.6",
    "angular-aria": "^1.6.6",
    "angular-cookies": "^1.6.6",
    "angular-messages": "^1.6.6",
    "angular-sanitize": "^1.6.6",
    "angular-touch": "^1.6.6",
    "angular-translate": "^2.13.0",
    "angular-translate-loader-static-files": "^2.13.0",
    "angular-ui-bootstrap": "^2.2.0",
    "angular-ui-router": "^1.0.0-beta.3",
    "bootstrap": "^3.3.7",
    "brace": "^0.10.0",
    "canvas-gauges": "latest",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.2",
    "jquery-knob": "latest",
    "jquery-sticky": "^1.0.4",
    "oclazyload": "latest",
    "semantic-ui": "^2.2.13"
  },
  "devDependencies": {
    "angular-mocks": "^1.6.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.24.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "csslint": "^1.0.4",
    "eslint": "^3.9.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "fork-ts-checker-webpack-plugin": "latest",
    "html-webpack-plugin": "^2.7.1",
    "htmlhint": "^0.9.13",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.2.0",
    "ng-annotate-loader": "^0.2.0",
    "npm-run-all": "^3.1.2",
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.3.3",
    "typescript": "latest",
    "webpack": "^3.5.5",
    "webpack-angular-dgeni-plugin": "^0.0.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^0.14.1"
  }
}
